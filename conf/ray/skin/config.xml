		<mode title="Bronze Style" name="bronze" method="doSkinBronze" icon="conf/ray/image/theme/bronze.png" impress="conf/ray/image/theme/bronzeX.png" laf="Metal" theme-class="com.laxcus.util.skin.BronzeMetalTheme"> 
