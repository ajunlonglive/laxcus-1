<?xml version="1.0" encoding="utf-8"?>

<!-- 图形界面颜色适配文件，用户根据自己机器语言自动选择自己选择  -->

<!--
	说明：
	1. title的文字出现在菜单和工具提示上
	2. normal的文字写入配置文件
	3. method的文字是鼠标点击触发方法，固化在WatchWindow的类中 
	4. laf(look and feel)，感知外观。大小写敏感，与系统保持一致！
	5. theme-class, 界面主题类, 只限"metal"类型的主题
-->

<config>

	<skin language="default" country="default">
		<mode title="Gray Style" name="gray" method="doSkinGray" icon="conf/ray/image/theme/gray.png" impress="conf/ray/image/theme/grayX.png" laf="Metal" theme-class="com.laxcus.util.skin.GrayMetalTheme"> 
			<![CDATA[ conf/ray/skin/metal_gray.txt ]]>
		</mode>
		<!--
		<mode title="Gray Blue Style" name="gray_blue" method="doSkinGrayBlue" icon="conf/ray/image/theme/gray.png" impress="conf/ray/image/theme/grayX.png" laf="Metal" theme-class="com.laxcus.util.skin.GrayBlueMetalTheme"> 
			<![CDATA[ conf/ray/skin/metal_gray_blue.txt ]]>
		</mode>
		-->
		<mode title="Drak Style" name="dark" method="doSkinDark" icon="conf/ray/image/theme/dark.png" impress="conf/ray/image/theme/darkX.png" laf="Metal" theme-class="com.laxcus.util.skin.DarkMetalTheme"> 
			<![CDATA[ conf/ray/skin/metal_dark.txt ]]>
